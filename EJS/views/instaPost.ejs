<body>
    <h1 style="text-align: center;">Instagram</h1>
    <h2>This is the post for user : <%= data.name %>
    </h2>
    <p>Followers = <%= data.followers %>
    </p>
    <p>Following = <%= data.following %>
    </p>

    <% for(let post of data.posts){%>

        <img src="<%= post.image%>" alt="some img">
        <p>

            Likes : <%= post.likes %> &nbsp; &nbsp;
                Comments : <%= post.comments %>
        </p>
        <% } %>
            <script src="/jsFolder/logic.js"></script>
            <%- include("includes/head.ejs") %>
</body>

</html>